}
.default-css-type5{
	overflow:hidden;position:relative;padding:24px 16px;
	.line-1{
		clear: both;
		.left-part{float: left;font-weight:bold;font-size:15px;}
		.right-part{float: right}
	}
	.line-2{
		clear: both;
		.left-part{float: left}
		.right-part{float: right;
			.custom span{font-weight:normal;font-size:15px;font-style:normal;}
		}
	}
	.line-3{
		clear: both;
		.left-part{float: left}
		.right-part{
			float: right;
			.custom span{font-weight:normal;font-size:15px;font-style:normal;}
		}
	}
}
